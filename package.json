    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --clearCache && jest --verbose",
    "eject": "react-scripts eject",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6006 -s public",
    "webpack-build": "yarn webpack --config webpack.config.js",
    "generate-component": "yarn plop --plopfile ./generators/plopfile.js component",
    "generate-redux": "yarn plop --plopfile ./generators/plopfile.js reduxActions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
