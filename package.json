    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-docs": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
